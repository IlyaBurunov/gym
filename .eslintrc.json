{
  "extends": ["react-app"],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "flowtype",
    "import",
    "jsx-a11y",
    "prettier",
    "react",
    "react-hooks",
    "@typescript-eslint"
  ],
  "rules": {
    // "react/no-typos": 0,
    "react/jsx-key": 2,
    "react-hooks/rules-of-hooks": "error",
    "react/require-render-return": 0
    // "@typescript-eslint/no-explicit-any": 1
  }
}
